[user]
    email = apatel762@users.noreply.github.com
    name = Arjun Patel

[core]
    editor = nvim

[color]
    ui = auto

[alias]
    # Fancy looking commit log
    hist = log --all --graph --decorate --oneline
    last = log --all --graph --decorate --oneline -10

    s = status --short --branch
    gsh = git show HEAD

[credential]
	helper = cache --timeout=43200

[push]
	default = matching
